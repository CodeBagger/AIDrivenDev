{
  "name": "scheduling-app",
  "version": "1.0.0",
  "description": "A scheduling application with React frontend and SQL Server Express backend",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@supabase/supabase-js": "^2.38.0",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["scheduling", "calendar", "react", "sql-server"],
  "author": "",
  "license": "MIT"
}
